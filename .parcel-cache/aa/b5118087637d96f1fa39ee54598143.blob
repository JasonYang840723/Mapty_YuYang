    <title>mapty // Map your workouts</title>
  </head>
  <body>
    <div class="sidebar">
      <img src="1af3aa76f60835dfd5a981b77232a9e3" alt="Logo" class="logo">
      <div class="sort__section">
        <div class="sort__section sort__section--state">
          <span>Workout type : All</span>
        </div>
        <div class="sort__section sort__section--number">
          <span>Number of workouts : &nbsp;</span>
        </div>
        <div class="sort__section sort__section--options">
          <ul class="sort__options">
            <li>Sort options :</li>
            <li><a href="">Speed</a></li>
            <li><a href="">Duration</a></li>
            <li><a href="">Distance</a></li>
          </ul>
          <div class="sort__hamburger">
            <input type="checkbox" class="sort__hamburger--checkbox" id="sort-toggle">
            <label for="sort-toggle" class="sort__hamburger--button">
              <span class="sort__hamburger--icon">&nbsp;</span>
            </label>
            <div class="sort__hamburger--background">&nbsp;</div>
            <nav class="sort__hamburger--nav sort__hamburger--nav-active">
              <ul class="sort__hamburger--list">
                <li class="sort__section--item">
                  <a href="#1" class="sort__hamburger--link">All types</a>
                </li>
                <li class="sort__section--item">
                  <a href="#2" class="sort__hamburger--link"> üèÉ‚Äç‚ôÇÔ∏è Running</a>
                </li>
                <li class="sort__section--item">
                  <a href="#3" class="sort__hamburger--link"> üö¥‚Äç‚ôÄ Cycling</a>
                </li>
                <li class="sort__section--item">
                  <a href="#4" class="sort__hamburger--link"> ‚≠êFavorites</a>
                </li>
              </ul>
            </nav>
          </div>
          <div class="sort__section__reset--button">
            <a href="#" class="reset">reset</a>
          </div>
        </div>
      </div>

      <ul class="workouts">
        <form class="form hidden">
          <div class="form__row">
            <label class="form__label">Type</label>
            <select class="form__input form__input--type">
              <option value="running">Running</option>
              <option value="cycling">Cycling</option>
            </select>
          </div>
          <div class="form__row">
            <label class="form__label">Distance</label>
            <input type="text" class="form__input form__input--distance" placeholder="km">
          </div>
          <div class="form__row">
            <label class="form__label">Duration</label>
            <input type="text" class="form__input form__input--duration" placeholder="min">
          </div>

          <div class="form__row">
            <label class="form__label">Cadence</label>
            <input type="text" class="form__input form__input--cadence" placeholder="step/min">
          </div>
          <div class="form__row form__row--hidden">
            <label class="form__label">Elev Gain</label>
            <input type="text" class="form__input form__input--elevation" placeholder="meters">
          </div>
          <button class="form__btn">OK</button>
        </form>
        <div class="workout__container">
          <ul class="dropdown hidden" id="dropdown">
            <li class="dropdown__items">
              <svg class="dropdown__icon" id="icon_item">
                <use xlink:href="9a6d342f679703f73ec66041c2eb043e"></use>
              </svg>
              <span class="dropdown__icon-name">Edit</span>
            </li>
            <li class="dropdown__items">
              <svg class="dropdown__icon" id="icon_item">
                <use xlink:href="87c986eca4609717635dfeb9a6b79325"></use>
              </svg>
              <span class="dropdown__icon-name">Delete</span>
            </li>
            <li class="dropdown__items">
              <svg class="dropdown__icon" id="icon_item">
                <use xlink:href="f185b5a914af5d01cacc572b8d05981d"></use>
              </svg>
              <span class="dropdown__icon-name">
                Add to
                <br>
                Favorites
              </span>
            </li>
          </ul>
          <li class="workout workout--running" data-id="1234567890">
            <h2 class="workout__title">Running on April 14</h2>
            <svg class="workout__icon--expand">
              <use xlink:href="6cbfacdbc16252f40954fdf8b1fab19d"></use>
            </svg>

            <div class="workout__details">
              <span class="workout__icon">üèÉ‚Äç‚ôÇÔ∏è</span>
              <span class="workout__value">5.2</span>
              <span class="workout__unit">km</span>
            </div>
            <div class="workout__details">
              <span class="workout__icon">‚è±</span>
              <span class="workout__value">24</span>
              <span class="workout__unit">min</span>
            </div>
            <div class="workout__details">
              <span class="workout__icon">‚ö°Ô∏è</span>
              <span class="workout__value">4.6</span>
              <span class="workout__unit">min/km</span>
            </div>
            <div class="workout__details">
              <span class="workout__icon">ü¶∂üèº</span>
              <span class="workout__value">178</span>
              <span class="workout__unit">spm</span>
            </div>
          </li>
        </div>
        <div class="workout__container">
          <ul class="dropdown hidden" id="dropdown">
            <li class="dropdown__items">
              <svg class="dropdown__icon" id="icon_item">
                <use xlink:href="9a6d342f679703f73ec66041c2eb043e"></use>
              </svg>
              <span class="dropdown__icon-name">Edit</span>
            </li>
            <li class="dropdown__items">
              <svg class="dropdown__icon" id="icon_item">
                <use xlink:href="87c986eca4609717635dfeb9a6b79325"></use>
              </svg>
              <span class="dropdown__icon-name">Delete</span>
            </li>
            <li class="dropdown__items">
              <svg class="dropdown__icon" id="icon_item">
                <use xlink:href="f185b5a914af5d01cacc572b8d05981d"></use>
              </svg>
              <span class="dropdown__icon-name">
                Add to
                <br>
                Favorites
              </span>
            </li>
          </ul>
          <li class="workout workout--cycling" data-id="1234567891">
            <h2 class="workout__title">Cycling on April 5</h2>
            <svg class="workout__icon--expand">
              <use xlink:href="6cbfacdbc16252f40954fdf8b1fab19d"></use>
            </svg>
            <div class="workout__details">
              <span class="workout__icon">üö¥‚Äç‚ôÄÔ∏è</span>
              <span class="workout__value">27</span>
              <span class="workout__unit">km</span>
            </div>
            <div class="workout__details">
              <span class="workout__icon">‚è±</span>
              <span class="workout__value">95</span>
              <span class="workout__unit">min</span>
            </div>
            <div class="workout__details">
              <span class="workout__icon">‚ö°Ô∏è</span>
              <span class="workout__value">16</span>
              <span class="workout__unit">km/h</span>
            </div>
            <div class="workout__details">
              <span class="workout__icon">‚õ∞</span>
              <span class="workout__value">223</span>
              <span class="workout__unit">m</span>
            </div>
          </li>
        </div>
      </ul>
